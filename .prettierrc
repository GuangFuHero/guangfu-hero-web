{
  "tabWidth": 2,
  "printWidth": 100,
  "singleQuote": true,
  "endOfLine": "lf",
  "useTabs": false,
  "semi": true,
  "quoteProps": "as-needed",
  "trailingComma": "es5",
  "bracketSpacing": true,
  "bracketSameLine": false,
  "arrowParens": "avoid",
  "embeddedLanguageFormatting": "auto",
  "singleAttributePerLine": false,
  "overrides": [
    {
      "files": ["**/*.json"],
      "options": {
        "singleQuote": false
      }
    }
  ]
}
